[dev:children]
aws-servers

[aws-servers:children]
jumpbox-servers
internal-servers

[jumpbox-servers:vars]
ansible_user=admin
ansible_ssh_private_key_file=~/.ssh/battleshed-aws

[jumpbox-servers]
jumpbox.battleshed.uk

[internal-servers:vars]
ansible_user=admin
ansible_ssh_common_args='-o ProxyCommand="ssh -W %h:%p -q username@jumpbox.battleshed.uk"'

[internal-servers:children]
master-servers
node-servers

[master-servers]
server.battleshed.uk

[node-servers]
node-0.battleshed.uk
node-1.battleshed.uk